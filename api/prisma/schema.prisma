generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model app_info {
  id       Int       @id
  name     String?
  version  String?
  added_at DateTime? @db.Timestamptz(6)
}

model users {
  id       Int       @id @default(autoincrement())
  username String    @unique
  password String?
  added_at DateTime @default(now()) @db.Date
}